            <items jcr:primaryType="cq:WidgetCollection">
                <event
                    jcr:primaryType="cq:Widget"
                    path="/apps/nhmwww/components/page/eventdetailpage/tab_event.infinity.json"
                    xtype="cqinclude"/>

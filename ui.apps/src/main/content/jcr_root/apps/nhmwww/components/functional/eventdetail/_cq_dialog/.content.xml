        	<pricetab
        		jcr:primaryType="nt:unstructured"
				jcr:title="Event Prices"
				sling:resourceType="granite/ui/components/foundation/section">
				<layout 
					jcr:primaryType="nt:unstructured" 
					margin="{Boolean}false"
					sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" />
				<items jcr:primaryType="nt:unstructured">
					<column
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/foundation/container">
						<items jcr:primaryType="nt:unstructured">
							<adult
			                    jcr:primaryType="nt:unstructured"
			                    fieldLabel="Adult *"
			                    name="./adultPrice"
			                    sling:resourceType="granite/ui/components/foundation/form/textfield"
			                    class="required-price" >
			                </adult>
			                <concession
			                    jcr:primaryType="nt:unstructured"
			                    fieldLabel="Concession *"
			                    name="./concessionPrice"
			                    sling:resourceType="granite/ui/components/foundation/form/textfield"
			                    class="required-price" >
			                </concession>
			                <member
			                    jcr:primaryType="nt:unstructured"
			                    fieldLabel="Member *"
			                    name="./memberPrice"
			                    sling:resourceType="granite/ui/components/foundation/form/textfield"
			                    class="required-price" >
			                </member>
			                <family
			                    jcr:primaryType="nt:unstructured"
			                    fieldLabel="Family *"
			                    name="./familyPrice"
			                    sling:resourceType="granite/ui/components/foundation/form/textfield"
			                    class="required-price" >
			                </family>
			                <custom
			                    jcr:primaryType="nt:unstructured"
			                    fieldLabel="Custom *"
			                    name="./customPrice"
			                    sling:resourceType="granite/ui/components/foundation/form/textfield"
			                    class="required-price" >
			                </custom>
			                <eventListing
			                    jcr:primaryType="nt:unstructured"
			                    required="{Boolean}true"
			                    fieldLabel="Event Listing Price *"
			                    name="./eventListingPrice"
			                    sling:resourceType="granite/ui/components/foundation/form/textfield">
			                </eventListing>
						</items>
					</column>
				</items>
        	</pricetab>
        	<imagetab
        		jcr:primaryType="nt:unstructured"
				jcr:title="Image Link Config"
				sling:resourceType="granite/ui/components/foundation/section">
				<layout 
					jcr:primaryType="nt:unstructured" 
					margin="{Boolean}false"
					sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" />
				<items jcr:primaryType="nt:unstructured">
					<column
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/foundation/container">
						<items jcr:primaryType="nt:unstructured">
							<ctaLink
			                	jcr:primaryType="nt:unstructured"
								fieldLabel="CTA Link"
								name="./ctaLink"
								sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
								rootPath="/content/nhmwww" />
							<ctaText
			                    jcr:primaryType="nt:unstructured"
			                    fieldLabel="CTA Text"
			                    name="./ctaText"
			                    sling:resourceType="granite/ui/components/foundation/form/textfield" />
		                    <image
		                       jcr:primaryType="nt:unstructured"
		                       autoStart="{Boolean}false"
		                       class="cq-droptarget"
		                       fieldLabel="Image asset"
		                       fieldNameParameter="./fileName"
		                       fileReferenceParameter="./fileReference"
		                       mimeTypes="[image]"
		                       multiple="{Boolean}false"
		                       name="./file"
		                       sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
		                       title="Image"
		                       uploadUrl="${suffix.path}"
		                       useHTML5="{Boolean}true" />
						</items>
					</column>
				</items>
        	</imagetab>
        	<schooltabcontainer jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/foundation/container"
				jcr:title="School Config"
				class="cq-dialog-container-showhide"
				id="schooltabcontainer" >
				<items jcr:primaryType="nt:unstructured">
		        	<schooltab
		        		jcr:primaryType="nt:unstructured"
						jcr:title="School Conifig"
						sling:resourceType="granite/ui/components/foundation/section">

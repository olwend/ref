<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Carousel"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,cq.siteadmin.admin.properties]">
    <content 
    	jcr:primaryType="nt:unstructured"
    	sling:resourceType="granite/ui/components/foundation/container">
		<layout
			jcr:primaryType="nt:unstructured"
			sling:resourceType="granite/ui/components/foundation/layouts/tabs"
			type="nav" />
		<items jcr:primaryType="nt:unstructured">
			<carousel
				jcr:primaryType="nt:unstructured"
				jcr:title="Carousel Options"
				sling:resourceType="granite/ui/components/foundation/section">
				<layout
					jcr:primaryType="nt:unstructured"
					margin="{Boolean}false"
					sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" />
				<items jcr:primaryType="nt:unstructured">
					<column
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/foundation/container">
						<items jcr:primaryType="nt:unstructured">
							<fieldConfig
		                       jcr:primaryType="nt:unstructured"
		                       fieldLabel="Component title"
		                       name="./title"
		                       sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
		                   	<hyperlinkcontainer
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/container"
								class="showhide-target"
								id="hide-hyperlink-field">
								<items jcr:primaryType="nt:unstructured">
			                       	<hyperlink
					                   	jcr:primaryType="nt:unstructured"
										fieldLabel="Hyperlink"
										name="./hyperlink"
										sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
										rootPath="/content/nhmwww" />
								</items>
							</hyperlinkcontainer>
							<newwindow 
			                   	jcr:primaryType="nt:unstructured"
								name="./newwindow"
								sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
								text="Open in new window"
								value="{Boolean}true" />
							<grouping
			                    jcr:primaryType="nt:unstructured"
			                    fieldLabel="Grouping"
			                    defaultValue="1"
			                    name="./grouping"
								sling:resourceType="granite/ui/components/coral/foundation/form/select">
			                    <items jcr:primaryType="nt:unstructured">
			                        <o1
		                                jcr:primaryType="nt:unstructured"
		                                text="1"
		                                value="1"/>
		                            <o2
		                                jcr:primaryType="nt:unstructured"
		                                text="2"
		                                value="2"/>
		                            <o3
		                                jcr:primaryType="nt:unstructured"
		                                text="3"
		                                value="3"/>
			                    </items>
		                    </grouping>
		                    <thumbnails
								jcr:primaryType="nt:unstructured"
	                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
	                            text="Thumbnails"
	                            name="./thumbnails"
	                            value="{Boolean}true"
	                            class="cq-dialog-checkbox-showhide"
	                            show-hide-target="hide-hyperlink-field"
	                            should-show-when-checked="false"/>
		                    <autoscroll		                    
		                    	jcr:primaryType="nt:unstructured"
								name="./autoscroll"
								sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
								text="Autoscroll"
								value="{Boolean}true" />
		                    <autoscrollduration
								jcr:primaryType="nt:unstructured"
								defaultValue="{Long}4"
								fieldDescription="Time in seconds until the next slide is shown"
								fieldLabel="Autoscroll Duration"
								name="./autoscrollduration"
								sling:resourceType="granite/ui/components/foundation/form/numberfield"/>
						</items>
					</column>
				</items>
			</carousel>
			<carouselItems
				jcr:primaryType="nt:unstructured"
				jcr:title="Items"
				sling:resourceType="granite/ui/components/foundation/section">
				<layout
					jcr:primaryType="nt:unstructured"
					margin="{Boolean}false"
					sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" />
				<items jcr:primaryType="nt:unstructured">
					<column
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/foundation/container">
						<items jcr:primaryType="nt:unstructured">
							<fieldset
		                        jcr:primaryType="nt:unstructured"
		                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
		                        <layout
		                            jcr:primaryType="nt:unstructured"
		                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
		                        <items jcr:primaryType="nt:unstructured">
		                            <column
		                                jcr:primaryType="nt:unstructured"
		                                sling:resourceType="granite/ui/components/foundation/container">
		                                <items jcr:primaryType="nt:unstructured">
		                                    <slide
		                                        jcr:primaryType="nt:unstructured"
		                                        sling:resourceType="granite/ui/components/foundation/form/multifield"
		                                        class="full-width">
		                                        <field
		                                            jcr:primaryType="nt:unstructured"
		                                            sling:resourceType="granite/ui/components/foundation/form/fieldset"
		                                            eaem-nested="NODE_STORE"
		                                            name="./carouselItems">
		                                            <layout
		                                                jcr:primaryType="nt:unstructured"
		                                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
		                                                method="absolute"/>
		                                            <items jcr:primaryType="nt:unstructured">
		                                                <column
		                                                    jcr:primaryType="nt:unstructured"
		                                                    sling:resourceType="granite/ui/components/foundation/container">
		                                                    <items jcr:primaryType="nt:unstructured">
			                                                	<item
												                   	jcr:primaryType="nt:unstructured"
																	fieldLabel="Item"
																	name="./item"
																	sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
																	rootPath="/content/dam/nhmwww" />
																<itemMobile
												                   	jcr:primaryType="nt:unstructured"
																	fieldLabel="Item mobile"
																	name="./itemMobile"
																	sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
																	rootPath="/content/dam/nhmwww" />
																<itemURL
												                   	jcr:primaryType="nt:unstructured"
																	fieldLabel="URL"
																	name="./itemURL"
																	sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
																	rootPath="/content/nhmwww" />
		                                                        <itemHeading
										                            jcr:primaryType="nt:unstructured"
										                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
										                            name="./itemHeading"
										                            fieldLabel="Heading"
										                            useFixedInlineToolbar="{Boolean}true" >
										                            <rtePlugins jcr:primaryType="nt:unstructured">
										                                <paraformat
														        			jcr:primaryType="nt:unstructured"
														        			features="*">
														       				<formats jcr:primaryType="cq:WidgetCollection">
														       					<nhm2
														       						description="Heading 2"
														       						jcr:primaryType="nt:unstructured"
														       						tag="h2" />
													      						<nhm3
														       						description="Heading 3"
														       						jcr:primaryType="nt:unstructured"
														       						tag="h3" />
													      						<nhm4
														       						description="Heading 4"
														       						jcr:primaryType="nt:unstructured"
														       						tag="h4" />
													      						<nhm5
														       						description="Heading 5"
														       						jcr:primaryType="nt:unstructured"
														       						tag="h5" />
													      						<p
														       						description="Paragraph"
														       						jcr:primaryType="nt:unstructured"
														       						tag="p" />
														       				</formats>
														       			</paraformat>
														       			<subsuperscript
														       				jcr:primaryType="nt:unstructured"
														       				features="*" />
														 				<styles
														 					jcr:primaryType="nt:unstructured"
														 					features="*">
														 					<styles jcr:primaryType="cq:WidgetCollection">
														 						<headingBox
														 							jcr:primaryType="nt:unstructured"
														 							cssName="heading-box"
														 							text="Heading box" />
																				<introText
																					jcr:primaryType="nt:unstructured"
														 							cssName="intro-text"
														 							text="Intro text" />
														 					</styles>
																		</styles>
																		<links
																			jcr:primaryType="nt:unstructured"
																			features="[modifylink,unlink]" />
																		<misctools
														       				jcr:primaryType="nt:unstructured"
														       				features="[specialchars]" >
														       				<specialCharsConfig jcr:primaryType="nt:unstructured">
										                                        <chars jcr:primaryType="nt:unstructured">
										                                            <copyright
										                                                jcr:primaryType="nt:unstructured"
										                                                entity="&amp;#169;"/>
										                                            <trademark
										                                                jcr:primaryType="nt:unstructured"
										                                                entity="&amp;#8482;"/>
										                                            <registered
										                                                jcr:primaryType="nt:unstructured"
										                                                entity="&amp;#174;"/>
										                                        </chars>
										                                    </specialCharsConfig>
														       			</misctools>
														      			<spellcheck
														       				jcr:primaryType="nt:unstructured"
														       				features="*" />
										                            </rtePlugins>
										                            <uiSettings jcr:primaryType="nt:unstructured">
										                                <cui jcr:primaryType="nt:unstructured">
										                                    <inline
										                                        jcr:primaryType="nt:unstructured"
										                                        toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,spellcheck#checktext,misctools#specialchars,#styles,#paraformat]">
										                                        <popovers jcr:primaryType="nt:unstructured">
										                                            <justify
										                                                jcr:primaryType="nt:unstructured"
										                                                items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
										                                                ref="justify"/>
										                                            <lists
										                                                jcr:primaryType="nt:unstructured"
										                                                items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
										                                                ref="lists"/>
										                                            <paraformat
										                                                jcr:primaryType="nt:unstructured"
										                                                items="paraformat:getFormats:paraformat-pulldown"
										                                                ref="paraformat"/>
										                                            <styles
										                                                jcr:primaryType="nt:unstructured"
										                                                items="styles:getStyles:styles-pulldown"
										                                                ref="styles"/>
										                                        </popovers>
										                                    </inline>
										                                </cui>
										                            </uiSettings>
										                        </itemHeading>
										                        <itemCaption
										                            jcr:primaryType="nt:unstructured"
										                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
										                            name="./itemCaption"
										                            fieldLabel="Caption"
										                            useFixedInlineToolbar="{Boolean}true" >
										                            <rtePlugins jcr:primaryType="nt:unstructured">
										                                <paraformat
														        			jcr:primaryType="nt:unstructured"
														        			features="*">
														       				<formats jcr:primaryType="cq:WidgetCollection">
														       					<nhm2
														       						description="Heading 2"
														       						jcr:primaryType="nt:unstructured"
														       						tag="h2" />
													      						<nhm3
														       						description="Heading 3"
														       						jcr:primaryType="nt:unstructured"
														       						tag="h3" />
													      						<nhm4
														       						description="Heading 4"
														       						jcr:primaryType="nt:unstructured"
														       						tag="h4" />
													      						<nhm5
														       						description="Heading 5"
														       						jcr:primaryType="nt:unstructured"
														       						tag="h5" />
													      						<p
														       						description="Paragraph"
														       						jcr:primaryType="nt:unstructured"
														       						tag="p" />
														       				</formats>
														       			</paraformat>
														       			<subsuperscript
														       				jcr:primaryType="nt:unstructured"
														       				features="*" />
														 				<styles
														 					jcr:primaryType="nt:unstructured"
														 					features="*">
														 					<styles jcr:primaryType="cq:WidgetCollection">
														 						<headingBox
														 							jcr:primaryType="nt:unstructured"
														 							cssName="heading-box"
														 							text="Heading box" />
																				<introText
																					jcr:primaryType="nt:unstructured"
														 							cssName="intro-text"
														 							text="Intro text" />
														 					</styles>
																		</styles>
																		<links
																			jcr:primaryType="nt:unstructured"
																			features="[modifylink,unlink]" />
																		<misctools
														       				jcr:primaryType="nt:unstructured"
														       				features="[specialchars]" >
														       				<specialCharsConfig jcr:primaryType="nt:unstructured">
										                                        <chars jcr:primaryType="nt:unstructured">
										                                            <copyright
										                                                jcr:primaryType="nt:unstructured"
										                                                entity="&amp;#169;"/>
										                                            <trademark
										                                                jcr:primaryType="nt:unstructured"
										                                                entity="&amp;#8482;"/>
										                                            <registered
										                                                jcr:primaryType="nt:unstructured"
										                                                entity="&amp;#174;"/>
										                                        </chars>
										                                    </specialCharsConfig>
														       			</misctools>
														      			<spellcheck
														       				jcr:primaryType="nt:unstructured"
														       				features="*" />
										                            </rtePlugins>
										                            <uiSettings jcr:primaryType="nt:unstructured">
										                                <cui jcr:primaryType="nt:unstructured">
										                                    <inline
										                                        jcr:primaryType="nt:unstructured"
										                                        toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,spellcheck#checktext,misctools#specialchars,#styles,#paraformat]">
										                                        <popovers jcr:primaryType="nt:unstructured">
										                                            <justify
										                                                jcr:primaryType="nt:unstructured"
										                                                items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
										                                                ref="justify"/>
										                                            <lists
										                                                jcr:primaryType="nt:unstructured"
										                                                items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
										                                                ref="lists"/>
										                                            <paraformat
										                                                jcr:primaryType="nt:unstructured"
										                                                items="paraformat:getFormats:paraformat-pulldown"
										                                                ref="paraformat"/>
										                                            <styles
										                                                jcr:primaryType="nt:unstructured"
										                                                items="styles:getStyles:styles-pulldown"
										                                                ref="styles"/>
										                                        </popovers>
										                                    </inline>
										                                </cui>
										                            </uiSettings>
										                        </itemCaption>
		                                                    </items>
		                                                </column>
		                                            </items>
		                                        </field>
		                                    </slide>
		                                </items>
		                            </column>
		                        </items>
		                    </fieldset>
						</items>
					</column>
				</items>
			</carouselItems>
		</items>
    </content>
</jcr:root>
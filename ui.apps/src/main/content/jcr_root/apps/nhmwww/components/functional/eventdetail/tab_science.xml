<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Panel"
    itemId="Science"
    title="Science Config">
    <items jcr:primaryType="cq:WidgetCollection">
        <basic
            jcr:primaryType="cq:Widget"
            collapsed="{Boolean}false"
            collapsible="{Boolean}false"
            xtype="dialogfieldset">
            <items jcr:primaryType="cq:WidgetCollection">
                <subjectScience
                    jcr:primaryType="cq:Widget"
                    cls="cq-propsdialog-tags"
                    fieldLabel="Subject"
                    name="./cq:subjectScience"
                    nhmTagsBasePaths="[/etc/tags/nhmwww/audience,/etc/tags/nhmwww/events,/etc/tags/nhmwww/key-stage,/etc/tags/nhmwww/subject]"
                    xtype="nhmtags"/>
                <speakerDetails
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Speaker Details"
                    name="./speakerDetails"
                    xtype="textfield"/>
            </items>
        </basic>
    </items>
</jcr:root>

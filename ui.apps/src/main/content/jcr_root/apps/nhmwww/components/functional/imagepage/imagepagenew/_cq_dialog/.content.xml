                                                class="full-width"
                                                eaem-nested=""
                                                fieldDescription="Click '+' to add an item"
                                                fieldLabel="Image page item">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                    name="./imagepageitems">
                                                    <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                        method="absolute"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <components
												                    jcr:primaryType="nt:unstructured"
												                    fieldLabel="Components"
												                    defaultValue="imageandtext"
												                    name="./components"
																	sling:resourceType="granite/ui/components/foundation/form/select"
											                        class="cq-dialog-select-showhide"
											                        show-hide-target="show-filter-select"
											                        id="filterSelection" >
												                    <items jcr:primaryType="nt:unstructured">
												                    	<o1
											                                jcr:primaryType="nt:unstructured"
											                                text="Image and text"
											                                value="imageandtext"/>					                            
											                            <o2
											                                jcr:primaryType="nt:unstructured"
											                                text="Text"
											                                value="text"/>
										                               	<o3
											                                jcr:primaryType="nt:unstructured"
											                                text="2 images"
											                                value="twoimages"/>
											                            <o4
											                                jcr:primaryType="nt:unstructured"
											                                text="Pull out image"
											                                value="pulloutimage"/>
																	</items>
																</components>
																<layoutContainer
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/foundation/container"
																	class="hidden showhide-target"
													                id="select-imageandtext">
																	<items jcr:primaryType="nt:unstructured">
																		<layout
														                    jcr:primaryType="nt:unstructured"
														                    fieldLabel="Row layout"
														                    defaultValue="herosection"
														                    name="rowlayout"
																			sling:resourceType="granite/ui/components/coral/foundation/form/select">
														                    <items jcr:primaryType="nt:unstructured">
														                    	<o1
													                                jcr:primaryType="nt:unstructured"
													                                text="Hero"
													                                value="herosection"/>
														                        <o2
													                                jcr:primaryType="nt:unstructured"
													                                text="Baby hero"
													                                value="babyherosection"/>					                            
													                            <o3
													                                jcr:primaryType="nt:unstructured"
													                                text="Full width"
													                                value="rowfullwidth"/>
																			</items>
																		</layout>
																	</items>
																</layoutContainer>
